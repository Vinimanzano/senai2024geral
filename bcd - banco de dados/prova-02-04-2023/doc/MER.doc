MER

veiculo(
    placa: VARCHAR(15) PRIMARY KEY UNIQUE, 
    modelo: VARCHAR(30) NOT NULL, 
    marca: VARCHAR(20) NOT NULL, 
    tipo: EMUN('Standart', 'Esportivo', 'Utilitario') NOT NULL
    );

cliente(
    cli_id: INTEGER PRIMARY KEY AUTO_INCREMENT, 
    nome: VARCHAR(100) NOT NULL, 
    cpf:VARCHAR(15) NOT NULL UNIQUE
);

telefone(
    tel_id: INTEGER PRIMARY KEY AUTO_INCREMENT, 
    tel_primario: VARCHAR(25) NOT NULL, 
    tel_secundario: VARCHAR(25) NOT NULL, 
    cliente: INTEGER REFERENCES[cli_id]
);

reserva(
    res_id: INTEGER PRIMARY KEY AUTO_INCREMENT,
    data_reserva: DATE NOT NULL,
    data_retirada: DATE NOT NULL,
    data_devolucao: DATE NOT NULL,
    dias: INTEGER NOT NULL,
    status: EMUN('Concluido', 'Alugado' 'Reservado') NOT NULL DEFALT 'Em Espera' NOT NULL,
    diaria: DECIMAL(7,2) NOT NULL,
    sub_total: DECIMAL(10,2) NOT NULL,
    placa: VARCHAR(15) REFERENCES[veiculo: placa],
    cliente: INTEGER REFERENCES[cliente: cli_id]
);
